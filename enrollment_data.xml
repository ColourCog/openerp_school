<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

    <!-- Sequences for school.roster.enrollment -->
    <record id="seq_type_school_roster_enrollment" model="ir.sequence.type">
        <field name="name">Roster enrollment</field>
        <field name="code">school.roster.enrollment</field>
    </record>

    <record id="seq_school_roster_enrollment" model="ir.sequence">
        <field name="name">Roster enrollment</field>
        <field name="code">school.roster.enrollment</field>
        <field name="prefix">SRE</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="mt_reg_draft" model="mail.message.subtype">
      <field name="name">enrollment Draft</field>
      <field name="res_model">school.roster.enrollment</field>
      <field name="default" eval="False"/>
      <field name="description">enrollment Draft created</field>
    </record>

    <record id="mt_reg_pending" model="mail.message.subtype">
      <field name="name">enrollment Pending</field>
      <field name="res_model">school.roster.enrollment</field>
      <field name="default" eval="False"/>
      <field name="description">enrollment Pending</field>
    </record>

    <record id="mt_reg_cancelled" model="mail.message.subtype">
      <field name="name">enrollment Cancelled</field>
      <field name="res_model">school.roster.enrollment</field>
      <field name="default" eval="False"/>
      <field name="description">enrollment Cancelled</field>
    </record>

    <record id="mt_reg_done" model="mail.message.subtype">
      <field name="name">enrollment Complete</field>
      <field name="res_model">school.roster.enrollment</field>
      <field name="default" eval="False"/>
      <field name="description">enrollment Complete</field>
    </record>


    </data>
</openerp>
