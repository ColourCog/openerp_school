<openerp>
    <data noupdate="1">

        <!-- Student-related subtypes for messaging / Chatter -->
        <record id="mt_reg_registered" model="mail.message.subtype">
            <field name="name">Student Registered</field>
            <field name="res_model">school.registration</field>
            <field name="default" eval="False"/>
            <field name="description">Student registered</field>
        </record>
        <record id="mt_reg_cancelled" model="mail.message.subtype">
            <field name="name">Registration Cancelled</field>
            <field name="res_model">school.registration</field>
            <field name="default" eval="False"/>
            <field name="description">Registration cancelled</field>
        </record>

  </data>
</openerp>
