<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

    <!-- Sequences for school.roster.enrolment -->
    <record id="seq_type_school_roster_enrolment" model="ir.sequence.type">
        <field name="name">Roster Enrolment</field>
        <field name="code">school.roster.enrolment</field>
    </record>

    <record id="seq_school_roster_enrolment" model="ir.sequence">
        <field name="name">Roster Enrolment</field>
        <field name="code">school.roster.enrolment</field>
        <field name="prefix">SRE</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="mt_reg_draft" model="mail.message.subtype">
      <field name="name">Enrolment Draft</field>
      <field name="res_model">school.roster.enrolment</field>
      <field name="default" eval="False"/>
      <field name="description">Enrolment Draft created</field>
    </record>

    <record id="mt_reg_pending" model="mail.message.subtype">
      <field name="name">Enrolment Pending</field>
      <field name="res_model">school.roster.enrolment</field>
      <field name="default" eval="False"/>
      <field name="description">Enrolment Pending</field>
    </record>

    <record id="mt_reg_cancelled" model="mail.message.subtype">
      <field name="name">Enrolment Cancelled</field>
      <field name="res_model">school.roster.enrolment</field>
      <field name="default" eval="False"/>
      <field name="description">Enrolment Cancelled</field>
    </record>

    <record id="mt_reg_done" model="mail.message.subtype">
      <field name="name">Enrolment Complete</field>
      <field name="res_model">school.roster.enrolment</field>
      <field name="default" eval="False"/>
      <field name="description">Enrolment Complete</field>
    </record>


    </data>
</openerp>
